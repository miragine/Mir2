<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="create" namespace="/user" extends="struts-default">
		<action name="login" class="userAction" method="login">
			<result name="success" type="redirect">/appserver/index.jsp</result>
			<result name="error" type="redirect">/appserver/login.jsp</result>
		</action>
	</package>
</struts>
